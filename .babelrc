{
  "env": {
    "test": {
      "presets": [
        [
          "@babel/preset-env",
          {
            "loose": true
          }
        ]
      ],
      "plugins": [
        "add-module-exports"
      ]
    },
    "cjs": {
      "presets": [
        [
          "@babel/preset-env",
          {
            "loose": true
          }
        ]
      ],
      "plugins": [
        "add-module-exports"
      ]
    },
    "es": {
      "presets": [
        [
          "@babel/preset-env",
          {
            "loose": true,
            "modules": false
          }
        ]
      ]
    },
    "rollup": {
      "presets": [
        [
          "@babel/preset-env",
          {
            "loose": true,
            "modules": false
          }
        ]
      ],
      "plugins": [
        "@babel/plugin-external-helpers"
      ]
    }
  }
}
