{
    "source": "./src",
    "destination": "./docs",
    "includes": [
        "\\.js$"
    ],
    "excludes": [
        "\\.external-ecmascript\\.js$"
    ],
    "plugins": [{
        "name": "esdoc-standard-plugin",
        "option": {
            "title": "Chain",
            "lint": { "enable": false },
            "unexportedIdentifier": {"enable": false},
            "undocumentIdentifier": {"enable": true}
        }
    }]
}
